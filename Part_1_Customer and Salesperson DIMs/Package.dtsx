                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT ISNULL(sp.BusinessEntityID, 0) AS SalesRepID, ISNULL(p.FirstName, 'Unknown') AS FirstName, ISNULL(p.LastName, 'Unknown') AS LastName, ISNULL(EA.EmailAddress, 'No Email') AS Email, ISNULL(st.Name, 'No Territory') AS TerritoryName, ISNULL(st.[Group], 'No Group') 
