                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    c.CustomerID,
    c.StoreID, 
    s.Name AS StoreName,
    CASE 
        WHEN s.Name IS NOT NULL THEN 'Reseller'
        ELSE 'Individual'
    END AS CustomerType,
    COALESCE(s.Name, p.FirstName + ' ' + p.LastName) AS CustomerName,
    a.AddressLine1 + ', ' + a.City + ', ' + sp.Name AS Address,
    EA.EmailAddress,
    c.AccountNumber,
    c.ModifiedDate AS LastUpdatedDate
FROM AdventureWorks2022.Sales.Customer AS c
LEFT JOIN AdventureWorks2022.Sales.Store AS s ON c.StoreID = s.BusinessEntityID 
LEFT JOIN AdventureWorks2022.Person.Person AS p ON c.PersonID = p.BusinessEntityID 
LEFT JOIN AdventureWorks2022.Person.BusinessEntityAddress AS BE ON c.CustomerID = BE.BusinessEntityID
LEFT JOIN AdventureWorks2022.Person.Address AS a ON BE.AddressID = a.AddressID
LEFT JOIN AdventureWorks2022.Person.StateProvince AS Sp ON a.StateProvinceID = Sp.StateProvinceID
LEFT JOIN AdventureWorks2022.Person.EmailAddress AS EA ON p.BusinessEntityID = EA.BusinessEntityID;</property>
